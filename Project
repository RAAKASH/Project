[ B1,C1,f1,A1,D1 ] = Datacompiler( l1,t ) ;
[ B2,C2,f2,A2,D2 ] = Datacompiler( l2,t ) ;
[b2]=Branch(l2,B2);           % Finds other branches
[b1]=Branch(l1,B1);
%%  x + l2(2)*cos(0 +B2(:,2)+pi/2) =  l1(2)*cos(B1(:,2)+pi/2)
